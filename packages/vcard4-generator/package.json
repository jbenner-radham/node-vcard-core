{
  "name": "@vcard/vcard4-generator",
  "version": "0.0.0",
  "description": "A vCard 4 generator library.",
  "keywords": [
    "creator",
    "generator",
    "make",
    "vcard",
    "vcard4",
    "vcf"
  ],
  "homepage": "https://github.com/jbenner-radham/node-vcard-core#readme",
  "bugs": {
    "url": "https://github.com/jbenner-radham/node-vcard-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbenner-radham/node-vcard-core.git"
  },
  "license": "MIT",
  "author": "James Benner <james.benner@gmail.com> (http://www.jamesbenner.com/)",
  "main": "dist/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "prebuild": "npm run typecheck && npm run clean",
    "build": "npm run build:js && npm run build:types",
    "build:js": "babel --root-mode upward --out-dir dist lib --extensions '.ts' --ignore '**/*.d.ts'",
    "build:types": "tsc --project './tsconfig.d.json'",
    "clean": "rimraf ./dist",
    "lint": "eslint lib test",
    "lint:fix": "eslint --fix  lib test",
    "make:property": "node ./scripts/make-property.cjs",
    "print:property": "node ./scripts/print-property.cjs",
    "sort:pkg": "sort-package-json",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "typecheck": "tsc"
  },
  "dependencies": {
    "core-js": "^3.19.2",
    "lodash.isplainobject": "^4.0.6",
    "lodash.kebabcase": "^4.1.1"
  },
  "devDependencies": {
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "handlebars": "^4.7.7",
    "lodash": "^4.17.21"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  }
}
